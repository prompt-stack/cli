{
  "name": "@learnrudi/db",
  "version": "1.0.2",
  "description": "Database layer for RUDI - tracks installs, runs, and artifacts",
  "type": "module",
  "main": "src/index.js",
  "exports": {
    ".": "./src/index.js",
    "./import": "./src/import.js",
    "./logs": "./src/logs.js",
    "./schema": "./src/schema.js",
    "./search": "./src/search.js",
    "./stats": "./src/stats.js",
    "./service": "./src/service/index.ts",
    "./service/turns": "./src/service/turns.ts",
    "./service/file-versions": "./src/service/file-versions.ts"
  },
  "scripts": {
    "test": "node --test src/__tests__/unit/*.test.js",
    "test:unit": "node --test src/__tests__/unit/*.test.js",
    "test:integration": "node --test src/__tests__/integration/*.test.js",
    "test:all": "node --test src/__tests__/unit/*.test.js src/__tests__/integration/*.test.js"
  },
  "dependencies": {
    "@learnrudi/env": "^1.0.0",
    "better-sqlite3": "^12.5.0",
    "isomorphic-git": "^1.36.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {},
  "engines": {
    "node": ">=18.0.0"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
